{
    "required_files": [
        ".gitattributes",
        ".gitignore",
        ".pre-commit-config.yaml",
        "DEVELOPER.md",
        "LICENSE",
        "README.md",
        "runtime_config.yaml"
    ],
    "pwa_project_files": [
        "index.html",
        "manifest.json",
        "sw.js"
    ],
    "node_project_files": [
        "package.json"
    ],
    "python_project_files": [
        "pyproject.toml"
    ],
    "python_project_dirs": [
        "src"
    ],
    "max_python_file_length": 1000,
    "docs": {
        "docs_dir": "docs",
        "docs_api_dir": "api"
    },
    "log_subdir": "logs",
    "log_file_template": "{time:YYYY-MM-DD}.log",
    "log_level": "INFO",
    "log_retention_days": 7
}
